<?xml version="1.0" encoding="UTF-8"?>
<Interface xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../Interface.xsd">

	<Windows>
		<Window name="EnemyScenarioAlerterConfiguration">
			<Windows>
				
				<!-- Content: enabled -->
				<Button name="$parentEnabled" inherits="EA_Button_DefaultCheckBox">
					<Anchors>
						<Anchor point="topleft" relativePoint="topleft">
							<AbsPoint x="20" y="0" />
						</Anchor>
					</Anchors>
					<EventHandlers>
						<EventHandler event="OnLButtonUp" function="Enemy.ScenarioAlerterUI_ConfigDialog_OnEnabledChanged" />
					</EventHandlers>
				</Button>
				<Label name="$parentEnabledLabel" font="font_chat_text">
					<Size>
						<AbsPoint x="400" y="30" />
					</Size>
					<Anchors>
						<Anchor point="right" relativePoint="left" relativeTo="$parentEnabled">
							<AbsPoint x="10" y="5" />
						</Anchor>
					</Anchors>
					<Color r="255" g="255" b="255" a="255" />
				</Label>
				
			</Windows>
		</Window>
	</Windows>
</Interface>