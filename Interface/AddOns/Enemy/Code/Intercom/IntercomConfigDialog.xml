<?xml version="1.0" encoding="UTF-8"?>
<Interface xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../Interface.xsd">
	<Windows>
		<Window name="EnemyIntercomConfiguration">
			<Windows>
			
				<!-- Content: private -->
				<Button name="$parentPrivate" inherits="EA_Button_DefaultCheckBox">
					<Anchors>
						<Anchor point="topleft" relativePoint="topleft">
							<AbsPoint x="20" y="0" />
						</Anchor>
					</Anchors>
					<EventHandlers>
						<EventHandler event="OnLButtonUp" function="Enemy.IntercomUI_ConfigDialog_OnPrivateChanged" />
					</EventHandlers>
				</Button>
				<Label name="$parentPrivateLabel" font="font_chat_text">
					<Size>
						<AbsPoint x="600" y="30" />
					</Size>
					<Anchors>
						<Anchor point="right" relativePoint="left" relativeTo="$parentPrivate">
							<AbsPoint x="10" y="5" />
						</Anchor>
					</Anchors>
					<Color r="255" g="255" b="255" a="255" />
				</Label>
				
			</Windows>
		</Window>
	</Windows>
</Interface>