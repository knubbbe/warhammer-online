<Interface xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../Interface.xsd">

	<Windows>

		<!-- ##### TAB HUD Buffs ############################################################################### -->
		<Window name="HGG_TabHUDBuffsWindowTemplate" inherits="HGG_TabWindowTemplate">
			<Windows>

                <!-- ===== Player (De)Buffs ==================================================================== -->
				<Label name="$parentPlayerBuffsFilterLabel" font="font_default_text_small" maxchars="32" textalign="rightcenter" layer="default" handleinput="false">
					<Size>
						<AbsPoint x="150" y="40" />
					</Size>
					<Anchors>
						<Anchor point="topleft" relativePoint="topleft" relativeTo="$parent">
							<AbsPoint x="0" y="0" />
						</Anchor>
					</Anchors>
				</Label>

				<ComboBox name="$parentPlayerBuffsFilter" inherits="EA_ComboBox_DefaultResizable">
					<Size>
						<AbsPoint x="250" y="28" />
					</Size>
					<Anchors>
						<Anchor point="right" relativePoint="left" relativeTo="$parentPlayerBuffsFilterLabel">
							<AbsPoint x="10" y="0" />
						</Anchor>
					</Anchors>
					<EventHandlers>
						<EventHandler event="OnSelChanged" function="HealGridGuiTabHUDBuffs.ChangePlayerBuffsFilter" />
					</EventHandlers>
				</ComboBox>

				<Label name="$parentPlayerDebuffsFilterLabel" font="font_default_text_small" maxchars="32" textalign="rightcenter" layer="default" handleinput="false">
					<Size>
						<AbsPoint x="150" y="40" />
					</Size>
					<Anchors>
						<Anchor point="topleft" relativePoint="topleft" relativeTo="$parent">
							<AbsPoint x="0" y="40" />
						</Anchor>
					</Anchors>
				</Label>

				<ComboBox name="$parentPlayerDebuffsFilter" inherits="EA_ComboBox_DefaultResizable">
					<Size>
						<AbsPoint x="250" y="28" />
					</Size>
					<Anchors>
						<Anchor point="right" relativePoint="left" relativeTo="$parentPlayerDebuffsFilterLabel">
							<AbsPoint x="10" y="0" />
						</Anchor>
					</Anchors>
					<EventHandlers>
						<EventHandler event="OnSelChanged" function="HealGridGuiTabHUDBuffs.ChangePlayerDebuffsFilter" />
					</EventHandlers>
				</ComboBox>

                <!-- ===== Friendly Target (De)Buffs =========================================================== -->
				<Label name="$parentFriendlyTargetBuffsFilterLabel" font="font_default_text_small" maxchars="32" textalign="rightcenter" layer="default" handleinput="false">
					<Size>
						<AbsPoint x="150" y="40" />
					</Size>
					<Anchors>
						<Anchor point="topleft" relativePoint="topleft" relativeTo="$parent">
							<AbsPoint x="0" y="120" />
						</Anchor>
					</Anchors>
				</Label>

				<ComboBox name="$parentFriendlyTargetBuffsFilter" inherits="EA_ComboBox_DefaultResizable">
