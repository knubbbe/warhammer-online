					<Size>
						<AbsPoint x="150" y="40" />
					</Size>
					<Anchors>
						<Anchor point="topleft" relativePoint="topleft" relativeTo="$parent">
							<AbsPoint x="0" y="160" />
						</Anchor>
					</Anchors>
				</Label>

				<ComboBox name="$parentFriendlyTargetDebuffsFilter" inherits="EA_ComboBox_DefaultResizable">
