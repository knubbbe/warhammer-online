					<EventHandlers>
						<EventHandler event="OnSlide" function="HealGridGuiTabHUDBuffs.ShowBuffsMaxChanged" />
					</EventHandlers>
				</SliderBar>

                <!-- ===== Buff Frame Size Adjust ============================================================== -->
				<Label name="$parentBuffFrameSizeAdjustLabel" inherits="HGG_ItemLabelTemplate">
					<Anchors>
						<Anchor point="top" relativePoint="topleft" relativeTo="$parent">
