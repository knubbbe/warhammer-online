                <!-- ===== Show Buffs Max ====================================================================== -->
				<Label name="$parentShowBuffsMaxLabel" inherits="HGG_ItemLabelTemplate">
					<Anchors>
						<Anchor point="top" relativePoint="topleft" relativeTo="$parent">
							<AbsPoint x="0" y="0" />
						</Anchor>
					</Anchors>
				</Label>

				<SliderBar name="$parentShowBuffsMaxSlider" inherits="HGG_SliderFullLabels" numticks="10">
					<Size>
						<AbsPoint x="250" y="33" />
					</Size>
					<Anchors>
						<Anchor point="right" relativePoint="left" relativeTo="$parentShowBuffsMaxLabel">
							<AbsPoint x="10" y="0" />
