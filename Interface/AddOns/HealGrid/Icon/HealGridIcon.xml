<Interface xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../Interface.xsd">
	<Assets>
		<Texture name="hg_healgridicon" file="Icon/healgridicon.tga" />
	</Assets>

	<Windows>

		<DynamicImage name="HG_HealGridIcon" handleinput="true" texture="hg_healgridicon" savesettings="true">
			<Size>
				<AbsPoint x="32" y="32" />
			</Size>
			<TexCoords x="0" y="0" />
			<TexDims x="24" y="24" />
			<Anchors>
				<Anchor point="left" relativePoint="right" relativeTo="EA_Window_OverheadMapMapDisplay">
					<AbsPoint x="5" y="10" />
				</Anchor>
			</Anchors>
			<EventHandlers>
				<EventHandler event="OnLButtonUp" function="HealGridGui.ToggleGui" />
			</EventHandlers>
		</DynamicImage>

	</Windows>

</Interface>
