<Interface xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../Interface.xsd">

	<Windows>

		<!-- ##### SkinEditor : Skin Attribute Label ########################################################### -->

		<Window name="HGSE_SkinAttributeBooleanTemplate" layer="secondary">

			<Anchors>
				<Anchor point="topleft" relativePoint="topleft" relativeTo="$parent" />
				<Anchor point="bottomright" relativePoint="bottomright" relativeTo="$parent">
					<AbsPoint x="0" y="-50" />
				</Anchor>
			</Anchors>

			<Windows>

				<!-- ===== Value ===== -->
				<Window name="$parentValue" inherits="HGG_LabelCheckButton">
					<Size>
						<AbsPoint x="200" y="30" />
					</Size>
					<Anchors>
						<Anchor point="topleft" relativePoint="topleft" relativeTo="$parent">
							<AbsPoint x="20" y="140" />
						</Anchor>
					</Anchors>
				</Window>

			</Windows>
		</Window>

    </Windows>

</Interface>

