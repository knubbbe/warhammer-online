<Interface xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../Interface.xsd">

	<Windows>

		<!-- ##### SkinEditor : Skin Attribute Anchor #################################################### -->

		<Window name="HGSE_SkinAttributeAnchorTemplate" layer="secondary">

			<Anchors>
				<Anchor point="topleft" relativePoint="topleft" relativeTo="$parent" />
				<Anchor point="bottomright" relativePoint="bottomright" relativeTo="$parent">
					<AbsPoint x="0" y="-50" />
				</Anchor>
			</Anchors>

			<Windows>

                <!-- ===== WindowName ===== -->
				<Label name="$parentLabelWindowName" font="font_default_text_small" textalign="rightcenter" text="Window">
					<Size>
						<AbsPoint x="80" y="32" />
					</Size>
					<Anchors>
						<Anchor point="topleft" relativePoint="topleft" relativeTo="$parent">
							<AbsPoint x="20" y="100" />
						</Anchor>
					</Anchors>
				</Label>

                <ComboBox name="$parentValueWindowName" inherits="EA_ComboBox_DefaultResizableSmall">
                    <Anchors>
                        <Anchor point="topleft" relativePoint="topleft" relativeTo="$parent">
                            <AbsPoint x="110" y="100" />
                        </Anchor>
                    </Anchors>
                </ComboBox>

                <!-- ===== RelativeTo ===== -->
				<Label name="$parentLabelRelativeTo" font="font_default_text_small" textalign="rightcenter" text="RelTo">
					<Size>
						<AbsPoint x="80" y="32" />
					</Size>
					<Anchors>
						<Anchor point="topleft" relativePoint="topleft" relativeTo="$parent">
							<AbsPoint x="240" y="100" />
						</Anchor>
					</Anchors>
				</Label>

                <ComboBox name="$parentValueRelativeTo" inherits="EA_ComboBox_DefaultResizableSmall">
                    <Anchors>
                        <Anchor point="topleft" relativePoint="topleft" relativeTo="$parent">
                            <AbsPoint x="330" y="100" />
                        </Anchor>
                    </Anchors>
                </ComboBox>

                <!-- ===== Point ===== -->
				<Label name="$parentLabelPoint" font="font_default_text_small" textalign="rightcenter" text="Point">
					<Size>
						<AbsPoint x="80" y="32" />
					</Size>
					<Anchors>
						<Anchor point="topleft" relativePoint="topleft" relativeTo="$parent">
							<AbsPoint x="20" y="140" />
						</Anchor>
					</Anchors>
				</Label>

                <ComboBox name="$parentValuePoint" inherits="EA_ComboBox_DefaultResizableSmall">
                    <Anchors>
                        <Anchor point="topleft" relativePoint="topleft" relativeTo="$parent">
                            <AbsPoint x="110" y="140" />
                        </Anchor>
                    </Anchors>
                </ComboBox>

                <!-- ===== RelativePoint ===== -->
				<Label name="$parentLabelRelativePoint" font="font_default_text_small" textalign="rightcenter" text="RelPnt">
					<Size>
						<AbsPoint x="80" y="32" />
					</Size>
					<Anchors>
						<Anchor point="topleft" relativePoint="topleft" relativeTo="$parent">
							<AbsPoint x="240" y="140" />
						</Anchor>
					</Anchors>
				</Label>

                <ComboBox name="$parentValueRelativePoint" inherits="EA_ComboBox_DefaultResizableSmall">
                    <Anchors>
                        <Anchor point="topleft" relativePoint="topleft" relativeTo="$parent">
                            <AbsPoint x="330" y="140" />
                        </Anchor>
                    </Anchors>
                </ComboBox>

                <!-- ===== OffsetX ===== -->
				<Label name="$parentLabelOffsetX" font="font_default_text_small" textalign="rightcenter" text="OffX">
					<Size>
						<AbsPoint x="80" y="32" />
					</Size>
					<Anchors>
						<Anchor point="topleft" relativePoint="topleft" relativeTo="$parent">
							<AbsPoint x="20" y="180" />
						</Anchor>
					</Anchors>
				</Label>

                <EditBox name="$parentValueOffsetX" inherits="EA_EditBox_DefaultFrame" history="15" maxchars="255">
                    <Size>
                        <AbsPoint x="100" y="32" />
                    </Size>
                    <Anchors>
                        <Anchor point="topleft" relativePoint="topleft" relativeTo="$parent">
                            <AbsPoint x="110" y="180" />
                        </Anchor>
                    </Anchors>
                </EditBox>

                <!-- ===== OffsetY ===== -->
				<Label name="$parentLabelOffsetY" font="font_default_text_small" textalign="rightcenter" text="OffY">
					<Size>
						<AbsPoint x="80" y="32" />
					</Size>
					<Anchors>
						<Anchor point="topleft" relativePoint="topleft" relativeTo="$parent">
							<AbsPoint x="240" y="180" />
						</Anchor>
					</Anchors>
				</Label>

                <EditBox name="$parentValueOffsetY" inherits="EA_EditBox_DefaultFrame" history="15" maxchars="255">
                    <Size>
                        <AbsPoint x="100" y="32" />
                    </Size>
                    <Anchors>
                        <Anchor point="topleft" relativePoint="topleft" relativeTo="$parent">
                            <AbsPoint x="330" y="180" />
                        </Anchor>
                    </Anchors>
                </EditBox>

			</Windows>
		</Window>

    </Windows>

</Interface>
