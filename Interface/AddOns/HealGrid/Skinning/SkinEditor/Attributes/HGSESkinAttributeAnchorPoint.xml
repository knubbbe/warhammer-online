<Interface xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../Interface.xsd">

	<Windows>

		<!-- ##### SkinEditor : Skin Attribute Anchor Point #################################################### -->

		<Window name="HGSE_SkinAttributeAnchorPointTemplate" layer="secondary">

			<Anchors>
				<Anchor point="topleft" relativePoint="topleft" relativeTo="$parent" />
				<Anchor point="bottomright" relativePoint="bottomright" relativeTo="$parent">
					<AbsPoint x="0" y="-50" />
				</Anchor>
			</Anchors>

			<Windows>

                <!-- ===== ComboBox: Value ===== -->
                <ComboBox name="$parentValue" inherits="EA_ComboBox_DefaultResizable">
                    <Anchors>
                        <Anchor point="topleft" relativePoint="topleft" relativeTo="$parent">
                            <AbsPoint x="20" y="100" />
                        </Anchor>
                    </Anchors>
                </ComboBox>

			</Windows>
		</Window>

    </Windows>

</Interface>

